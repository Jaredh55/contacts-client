<% @contacts.each do |contact| %>
  <h2><a href="/client/contacts/<%= contact["id"] %>"><%= contact["full_name"] %></a></h2>
  <h3> Middle Name: <%= contact["middle_name"] %></h3>
  <h4> Email: <%= contact["email"] %></h4>
  <h4> Phone: <%= contact["phone"] %></h4>
  <h4> Bio: <%= contact["bio"] %></h4>

  <br/>

<% end %>